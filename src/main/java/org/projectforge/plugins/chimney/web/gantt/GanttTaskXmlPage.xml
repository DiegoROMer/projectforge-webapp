<?xml version="1.0" encoding="UTF-8"?>
<project>
	<task wicket:id="taskRepeater">
		<pID wicket:id="taskId">[unique id]</pID>
		<pName wicket:id="taskName">[task name]</pName>
		<pCaption wicket:id="caption">[caption to show in the plot of the chart]</pCaption>
		<pStart wicket:id="startDate">[start date dd/mm/yyy]</pStart>
		<pEnd wicket:id="endDate">[end date dd/mm/yyy]</pEnd>
		<pColor wicket:id="backGroundColor">[color in hex-format: 0000ff]</pColor>
		<pLink wicket:id="taskLink">[any link showing up in a new window]</pLink>
		<pMile wicket:id="isMileStone">[is this a milestone? 0 = no, 1 = yes]</pMile>
		<pRes wicket:id="resourceName">[responsible resource]</pRes>
		<pComp wicket:id="progressPercentage">[progress in percent]</pComp>
		<pGroup wicket:id="isGroup">[has this task subtasks? 0 = no, 1 = yes]</pGroup>
		<pParent wicket:id="parentId">[unique id of parent task, if any]</pParent>
		<pOpen wicket:id="isExpanded">[is this group initially expanded? 0 = no, 1 = yes]</pOpen>
		<pDepend wicket:id="dependsOn">[unique ids of dependent tasks, optional relation type ee,eb,be,bb eg. "12eb 5 16bb"]</pDepend>
	</task>
</project>